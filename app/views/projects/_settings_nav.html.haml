%ul.project-settings-nav.sidebar-subnav
  = nav_link(path: 'projects#edit') do
    = link_to edit_project_path(@project), title: 'プロジェクト', class: "stat-tab tab " do
      %i.fa.fa-pencil-square-o
      %span
        プロジェクト
  = nav_link(controller: [:project_members, :teams]) do
    = link_to namespace_project_project_members_path(@project.namespace, @project), title: 'メンバー', class: "team-tab tab" do
      %i.fa.fa-users
      %span
        メンバー
  = nav_link(controller: :deploy_keys) do
    = link_to namespace_project_deploy_keys_path(@project.namespace, @project), title: 'デプロイキー' do
      %i.fa.fa-key
      %span
        デプロイキー
  = nav_link(controller: :hooks) do
    = link_to namespace_project_hooks_path(@project.namespace, @project), title: 'Webフック' do
      %i.fa.fa-link
      %span
        Webフック
  = nav_link(controller: :services) do
    = link_to namespace_project_services_path(@project.namespace, @project), title: 'サービス' do
      %i.fa.fa-cogs
      %span
        サービス
  = nav_link(controller: :protected_branches) do
    = link_to namespace_project_protected_branches_path(@project.namespace, @project), title: '保護ブランチ' do
      %i.fa.fa-lock
      %span
        保護ブランチ
